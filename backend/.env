# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/screenshot-extension

# JWT Secret (used to create authentication tokens)
JWT_SECRET=your-super-secret-key-change-this-later

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key-will-go-here
AWS_SECRET_ACCESS_KEY=your-aws-secret-key-will-go-here
AWS_REGION=us-east-1
AWS_BUCKET_NAME=screenshot-extension-bucket

# CORS (allowed origins - your chrome extension)
CORS_ORIGIN=chrome-extension://*